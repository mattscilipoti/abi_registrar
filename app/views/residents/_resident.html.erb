<div id="<%= dom_id resident %>">
  <p>
    <strong>Last name:</strong>
    <%= resident.last_name %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= resident.first_name %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= resident.email_address %>
  </p>

  <p>
    <strong>Verified at:</strong>
    <%= resident.verified_at %>
  </p>

  <p>
    <strong>Is deed holder:</strong>
    <%= resident.is_deed_holder %>
  </p>

  <p>
    <strong>Is renter:</strong>
    <%= resident.is_renter %>
  </p>

  <p>
    <strong>Is minor:</strong>
    <%= resident.is_minor %>
  </p>

  <p>
    <strong>Age of minor:</strong>
    <%= resident.age_of_minor %>
  </p>

</div>
